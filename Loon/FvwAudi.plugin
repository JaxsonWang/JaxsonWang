#!name=一汽奥迪
#!desc=获取一汽奥迪的 Cookie 和签到
#!author=淮城一只猫
#!homepage=https://github.com/JaxsonWang/JaxsonWang/tree/master/Scripts
#!icon=https://raw.githubusercontent.com/JaxsonWang/JaxsonWang/master/Assets/Color/Audi.png
#!category=Joiner

[MITM]
hostname = audi2c.faw-vw.com, audiclub.faw-vw.com

[Script]
# 获取一汽奥迪 Cookie
http-request ^https?:\/\/audi2c\.faw-vw\.com\/capi\/v2\/feed requires-body=0, script-path=https://raw.githubusercontent.com/JaxsonWang/JaxsonWang/master/Scripts/Joiner/fvw-audi.js, tag=获取一汽奥迪Cookie
# 一汽奥迪签到
cron "15 7 * * *" script-path=https://raw.githubusercontent.com/JaxsonWang/JaxsonWang/master/Scripts/Joiner/fvw-audi.js, tag=一汽奥迪签到
# 获取奥迪官方俱乐部 Cookie
http-request ^https?:\/\/audiclub\.faw-vw\.com\/getBuryConfigAndUserData requires-body=0, script-path=https://raw.githubusercontent.com/JaxsonWang/JaxsonWang/master/Scripts/Joiner/fvw-audi-club.js, tag=获取奥迪官方俱乐部Cookie
# 获取奥迪官方俱乐部签到
cron "20 7 * * *" script-path=https://raw.githubusercontent.com/JaxsonWang/JaxsonWang/master/Scripts/Joiner/fvw-audi-club.js, tag=获取奥迪官方俱乐部签到
